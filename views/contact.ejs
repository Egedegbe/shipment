<style>
    .contact_container {
        background: #f8f9fa;
        display: flex;
        justify-content: center;
        align-items: center;
        min-height: 100vh;
        padding: 20px;
    }
    .contact-container {
        background: #ffffff;
        padding: 40px;
        border-radius: 10px;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
        width: 100%;
        max-width: 600px;
    }
    .contact-title {
        font-size: 28px;
        margin-bottom: 20px;
        text-align: center;
        color: #333;
    }
    .form-control {
        margin-bottom: 15px;
        border-radius: 5px;
        border: 1px solid #ddd;
        padding: 15px;
        font-size: 16px;
        resize: none; /* For the message textarea */
    }
    .btn-submit {
        position: relative;
        background: #5a6c8f;
        border: none;
        padding: 12px 20px;
        color: #fff;
        font-size: 16px;
        font-weight: bold;
        border-radius: 5px;
        overflow: hidden;
        transition: all 0.3s ease;
        width: 100%;
        max-width: 100%;
        cursor: pointer;
    }
    .btn-submit:hover {
        background: #7b9bdb;
    }
    .btn-submit::before {
        content: '';
        position: absolute;
        width: 300%;
        height: 300%;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%) scale(0);
        background: rgba(255, 255, 255, 0.5);
        border-radius: 50%;
        transition: transform 0.5s ease;
    }
    .btn-submit:hover::before {
        transform: translate(-50%, -50%) scale(1);
    }
</style>
